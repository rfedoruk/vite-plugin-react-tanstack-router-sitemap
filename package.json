{
  "name": "vite-plugin-tanstack-router-sitemap",
  "version": "1.0.3",
  "description": "Vite plugin to generate sitemaps from TanStack Router route trees",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite-plugin",
    "sitemap",
    "tanstack",
    "router",
    "vite"
  ],
  "author": "Roman Fedoruk",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/rfedoruk/vite-plugin-react-tanstack-router-sitemap"
}
}
